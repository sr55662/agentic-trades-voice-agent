{
  "name": "agentic-trades-voice-agent",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p .",
    "start": "node dist/index.js",
    "migrate": "node-pg-migrate -j 1 -d \"$DATABASE_URL\" -m ./migrations",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "seed": "node dist/scripts/seed.js",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "purge:holds": "node scripts/purge_holds.ts"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "^3.637.0",
    "@fastify/cors": "^10.0.1",
    "@fastify/formbody": "^8.0.1",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/websocket": "^10.0.1",
    "dotenv": "^16.4.5",
    "fastify": "^5.0.0",
    "fastify-raw-body": "^4.3.0",
    "pg": "^8.12.0",
    "pino-pretty": "^13.0.0",
    "stripe": "^16.6.0",
    "twilio": "^5.2.4",
    "zod": "^3.23.8",
    "@openai/agents": "^0.1.0",
    "@openai/agents-extensions": "^0.1.0",
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/sdk-metrics": "^1.8.0",
    "@aws-sdk/client-secrets-manager": "^3.645.0",
    "@fastify/websocket": "^8"
  },
  "devDependencies": {
    "@types/node": "^22.5.4",
    "@types/pg": "^8.10.2",
    "@types/twilio": "^3.19.5",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "node-pg-migrate": "^7.6.2",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.2",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint-plugin-jsdoc": "^48.0.0",
    "vitest": "^2.0.0",
    "@vitest/coverage-v8": "^2.0.0"
  }
}
